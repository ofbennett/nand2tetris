 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=z1);
    Not(in=b,out=z2);
    Nand(a=z1,b=z2,out=out);
}
