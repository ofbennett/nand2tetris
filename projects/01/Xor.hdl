/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=z1);
    Or(a=a, b=b, out=z2);
    And(a=z1, b=z2, out=out);
}